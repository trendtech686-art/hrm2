model PurchaseReturnItem {
  systemId       String         @id
  returnId       String
  productId      String
  quantity       Int
  unitPrice      Decimal        @db.Decimal(15, 2)
  total          Decimal        @db.Decimal(15, 2)
  reason         String?
  purchaseReturn PurchaseReturn @relation(fields: [returnId], references: [systemId], onDelete: Cascade)

  @@map("purchase_return_items")
}
