model IdCounter {
  systemId       String   @id @default(uuid())
  entityType     String   @unique
  prefix         String
  currentValue   Int      @default(0)
  padding        Int      @default(3)
  updatedAt      DateTime @updatedAt
  businessPrefix String?  @default("") @db.VarChar(50)
  systemPrefix   String?  @default("") @db.VarChar(50)

  @@map("id_counters")
}
