model JobTitle {
  systemId    String     @id
  id          String     @unique
  name        String
  description String?
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
  createdBy   String?
  updatedBy   String?
  deletedAt   DateTime?
  isDeleted   Boolean    @default(false)
  employees   Employee[]

  @@map("job_titles")
}
